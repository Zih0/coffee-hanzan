{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@firebase/*": [
        "src/firebase/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@api/*": [
        "src/utils/api/*"
      ],
      "@constants/*": [
        "src/utils/constants/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
